<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Sierra Leone
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="999" Category="0" Condition="2" Name="Emergency Number"/>
    <EccEntry Ecc="019" Category="0" Condition="2" Name="Fir"/>
    <EccEntry Ecc="117" Category="0" Condition="2" Name="Ambulance"/>
    <EccEntry Ecc="112" Category="0" Condition="2" Name="Police"/>
</EccTable>